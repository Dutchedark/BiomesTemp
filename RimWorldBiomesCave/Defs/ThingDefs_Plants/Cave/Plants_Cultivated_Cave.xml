<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef ParentName="PlantBase">
    <defName>PaleBeets</defName>
    <label>pale beets</label>
    <description>A pale purple beet that has been genetically engineered to be able to burrow its roots through stone and survive off what little nutrients it can get in the caverns.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <nutrition>0.16</nutrition>
    </statBases>
    <graphicData>
      <texturePath>Plants/Crops/PaleBeets</texturePath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <selectable>true</selectable>
    <pathCost>10</pathCost>
    <ingestible>
    </ingestible>
    <plant>
      <fertilityMin>0.01</fertilityMin>
      <growMinGlow>0</growMinGlow>
      <growOptimalGlow>0</growOptimalGlow>
      <fertilitySensitivity>0</fertilitySensitivity>
      <dieIfLeafless>true</dieIfLeafless>
      <maxMeshCount>1</maxMeshCount>
      <harvestTag>Standard</harvestTag>
      <harvestedThingDef>RawPaleBeets</harvestedThingDef>
      <harvestYield>7</harvestYield>
      <sowTags>
        <li>Ground</li>
        <li>Hydroponic</li>
      </sowTags>
      <topWindExposure>0.1</topWindExposure>
      <growDays>3</growDays>
      <visualSizeRange>
        <min>0.3</min>
        <max>1.05</max>
      </visualSizeRange>
    </plant>
  </ThingDef>

