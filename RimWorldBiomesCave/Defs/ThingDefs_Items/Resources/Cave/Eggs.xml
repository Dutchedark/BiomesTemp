<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="EggFertBase">
        <defName>RWBEggJellyPotFertilized</defName>
        <label>jellypot egg (fert.)</label>
        <description>Fertilized Jellypot egg. If all goes well, it should hatch into a baby jellypot. It can be eaten raw, but it's much better cooked.</description>
        <graphicData>
            <texPath>Things/Item/Resource/AnimalProductRaw/EggOval</texPath>
            <color>(255,255,255)</color>
        </graphicData>
        <statBases>
            <MarketValue>15</MarketValue>
        </statBases>
        <comps>
            <li Class="CompProperties_Hatcher">
                <hatcherDaystoHatch>4.5</hatcherDaystoHatch>
                <hatcherPawn>RWBJellypotBug</hatcherPawn>
            </li>
        </comps>
    </ThingDef>
    
